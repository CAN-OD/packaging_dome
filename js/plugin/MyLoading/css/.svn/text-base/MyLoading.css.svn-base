/*弹出层基本样式*/
.wwbPop {width: 100%;height: 100%;min-width:500px;max-height:1024px;position: absolute;left:0;top:0;overflow: hidden;z-index: 999;}
.wwbPop_Mask {width: 100%;height: 100%;max-height:1000px;background-color: rgba(0, 0, 0, 0.61);opacity: 0.6;filter:alpha(opacity=20);}
