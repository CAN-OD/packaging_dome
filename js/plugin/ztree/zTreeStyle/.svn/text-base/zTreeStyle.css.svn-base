ul.tmpTargetzTree {background-color:#FFE6B0; opacity:0.8; filter:alpha(opacity=80)}

span.tmpzTreeMove_arrow {width:16px; height:16px; display: inline-block; padding:0; margin:2px 0 0 1px; border:0 none; position:absolute;
	background-color:transparent; background-repeat:no-repeat; background-attachment: scroll;
	background-position:-110px -80px; background-image:url("img/zTreeStandard.png"); *background-image:url("img/zTreeStandard.gif")}

ul.ztree.zTreeDragUL {margin:0; padding:0; position:absolute; width:auto; height:auto;overflow:hidden; background-color:#cfcfcf; border:1px #00B83F dotted; opacity:0.8; filter:alpha(opacity=80)}
.zTreeMask {z-index:10000; background-color:#cfcfcf; opacity:0.0; filter:alpha(opacity=0); position:absolute}
.r-ztree-box .ztree li span.button.center_docu,.r-ztree-box .ztree li span.button.roots_docu,.r-ztree-box .ztree li span.button.bottom_docu{
	display: none;
}
.r-ztree-box .ztree li span.button.ico_docu{
	        background-position: -130px -2px;
}
.ztree li .level1 .level1>span.button{
    background-image: url("../../../img/icon.png");
    background-position:-4px -70px;
}
.ztree li  .level1 .level1>span.button.ico_open {
    margin-right: 2px;
background-position: -6px -108px;
    vertical-align: top;
     height: 13px;
}
#tree .level0 .level0>span.button.ico_docu{
 
    background-position: -30px -71px;
  
}
#tree  li a.curSelectedNode {    width: 100%; background-color:#038dc0; color:#fff;  border:1px #d5d5d5 solid; opacity:0.8;}
#tree li.level0 a.curSelectedNode span.button.ico_open{
	margin-right:2px; background-position:-80px -93px; vertical-align:top; *vertical-align:middle
}
#tree li.level0 a.curSelectedNode span.button.ico_close{
	margin-right:2px; background-position:-80px -93px; vertical-align:top; *vertical-align:middle
}
#tree li.level1 a.curSelectedNode span.button.ico_open{
	margin-right:2px; background-position:-79px -109px; vertical-align:top; *vertical-align:middle
}
#tree li.level1 a.curSelectedNode span.button.ico_close{
	margin-right:2px; background-position:-53px -109px; vertical-align:top; *vertical-align:middle
}
#tree  .level2 a.curSelectedNode  span.button.ico_docu {
    background-position: -53px -93px;
}
.ztree li  .level1 .level1>span.button.ico_close {
		      margin-right: 2px;
    background-position: -30px -109px;
	 height: 13px;
   
    vertical-align: top;
}
.leftTreeProjectClick{
	    background-color: rgba(22, 113, 149, 0.5);
}
